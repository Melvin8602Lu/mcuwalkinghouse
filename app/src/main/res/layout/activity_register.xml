<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register"
    tools:layout_editor_absoluteY="25dp">

    <Button
        android:id="@+id/cho_stu"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginBottom="26dp"
        android:layout_marginEnd="57dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="57dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="245dp"
        android:text="學生"
        app:layout_constraintBottom_toTopOf="@+id/cho_have"
        app:layout_constraintEnd_toStartOf="@+id/cho_landlord"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/cho_have"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="231dp"
        android:layout_marginLeft="147dp"
        android:layout_marginStart="147dp"
        android:text="已有帳號?"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cho_login"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cho_stu"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/cho_landlord"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="57dp"
        android:layout_marginRight="38dp"
        android:layout_marginTop="245dp"
        android:text="房東"
        app:layout_constraintBottom_toTopOf="@+id/cho_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cho_stu"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/cho_login"
        android:layout_width="58dp"
        android:layout_height="40dp"
        android:layout_marginBottom="224dp"
        android:layout_marginEnd="119dp"
        android:layout_marginRight="119dp"
        android:text="登入"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cho_have"
        app:layout_constraintTop_toBottomOf="@+id/cho_landlord"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:text="回首頁"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>